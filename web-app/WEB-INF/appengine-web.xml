<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>iui-js</application>
    <version>18</version>

    <static-files>
        <include path="/**.gif" expiration="1d" />
        <include path="/**.png" expiration="1d" />
        <include path="/**.js" />
        <include path="/**.css"/>
        <include path="/**.html"/>
        <include path="/**.frag"/>
        <include path="/**.manifest"/>
        <exclude path="/**.gtpl" />
    </static-files>
 
