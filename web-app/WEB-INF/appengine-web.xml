<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>iui-js</application>
    <version>15</version>

    <static-files>
        <include path="/**.gif"/>
        <include path="/**.png"/>
        <include path="/**.js"/>
        <include path="/**.css"/>
        <include path="/**.html"/>
        <include path="/**.frag"/>
        <include path="/**.manifest"/>
        <exclude path="/WEB-INF/**.groovy" />
        <exclude path="**.gtpl" />
    </static-files>

</appengine-web-app>
